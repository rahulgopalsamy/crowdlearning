      <div class="row">
          <fieldset>
              <legend class="orange-text">New Class</legend>
        
          <div class="input-field col s12">
              <i class="material-icons prefix">label</i>
          <input name="classname" id="icon_prefix" type="text" class="validate" required>
          <label for="icon_prefix">Class Name</label>
        </div>
          
           <div class="row center orange-text">
                Note: The classtoken will be automatically generated. You can see it after creating your class! 
        </div>
            <div class="row center">
               <button class="btn waves-effect waves-light orange" type="submit">Submit
    <i class="material-icons right">send</i>
  </button>
              </div>
          </fieldset>
        </div>
    </form>
        
