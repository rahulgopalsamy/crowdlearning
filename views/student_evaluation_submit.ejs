<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8"/>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1.0"/>
  <title>CL | Evaluation</title>
  <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
  <link href="/css/materialize.css" type="text/css" rel="stylesheet" media="screen,projection"/>
  <link href="/css/common.css" type="text/css" rel="stylesheet" media="screen,projection"/>
  <link href="/css/student_evaluation.css" type="text/css" rel="stylesheet" media="screen,projection"/>
        <link rel="icon"  type="image/png" href="/img/crowdlearning.png">
<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.1.1/jquery.min.js"></script>
    <script  src="http://ajax.aspnetcdn.com/ajax/jquery.validate/1.15.0/jquery.validate.min.js"></script>
  <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.98.0/js/materialize.min.js"></script>
    <script type="text/javascript" src="https://www.gstatic.com/charts/loader.js"></script>
</head>
<body>
  <nav>
    <div class="nav-wrapper">
      <a id="brand-image" class="brand-logo">CrowdLearning</a>
        <ul id="nav-mobile" class="right hide-on-med-and-down">
            <li><a href="/<%=link%>/<%=classname%>/coursepage"><%= classname%></a></li>
        <li><a href="/<%= link%>/home">Course</a></li>
        <li><a href="/user/logout">Logout</a></li>
      </ul>
    </div>
  </nav>
<div class="container">

    <div class="row top-margin">
    <a href="/student/<%=classname%>/evaluate"><h5>Next Question -></h5></a>
    </div>
    
        <div class="row">
            
                      
                                    <div class="card z-depth-1">
                                       <div class="card-content">
                                           <span class="card-title">Question</span>
                                            <%- data.question%>
                                       </div>
                                    </div>
                                    <div class="card z-depth-1">
                                       <div class="card-content">
                                         <span class="card-title">Answer Choice</span>
                                         <% for(var i=0; i < data.options.length; i++) { %>
  
                        <p>
                          <input name="answer" type="radio" value="option1" id="<%-data.options[i]%>" disabled />
                          <label for="<%-data.options[i]%>"><%-data.options[i]%></label><br>
                        </p>
  
                                          <% } %>
                                    <br> <h6>Correct answer is <%- data.correctanswer%></h6>
                                                
                                       </div>
                                    </div> 

                                    <div class="card z-depth-1">
                                       <div class="card-content">
                                           <span class="card-title">Explanation</span>
                                            <%- data.explanation%>
                                       </div>
                                    </div>     
                     

                            </div> 
                      </div>
         </div> 
                          <script>
                                 $(document).ready(function() {
                                                    $('select').material_select();
                                                  });
                          </script>
    </div>    
    <script>
  (function(i,s,o,g,r,a,m){i['GoogleAnalyticsObject']=r;i[r]=i[r]||function(){
  (i[r].q=i[r].q||[]).push(arguments)},i[r].l=1*new Date();a=s.createElement(o),
  m=s.getElementsByTagName(o)[0];a.async=1;a.src=g;m.parentNode.insertBefore(a,m)
  })(window,document,'script','https://www.google-analytics.com/analytics.js','ga');

  ga('create', 'UA-92848140-1', 'auto');
  ga('send', 'pageview');

</script>  
</body>
</html>