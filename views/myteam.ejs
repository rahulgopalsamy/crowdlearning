    <div class="container">
      <% if(data.teamname){ %>
            <div class="card blue-grey darken-1 white-text center">
              <span class="card-title">Current team</span>
              <p>Team name: <%= data.teamname%>
              <br>
              Token: <%= data.token%></p>
            </div>
        <% } else{ %>
        <div class=" center card blue-grey darken-1 white-text">No current team</div>
  </div>
      <% } %>

